/dts-v1/;
#include "exynos4412.dtsi"

/ {
	model = "FriendlyARM TINY4412 board based on Exynos4412";
	compatible = "friendlyarm,tiny4412", "samsung,exynos4412";

	chosen {
		bootargs = "console=ttySAC0,115200n8 androidboot.console=ttySAC0 lcd=HD700 earlyprintk";
	};

	memory {
		reg = <0x40000000 0x40000000>;
	};

	rtc@10070000 {
		status = "okay";
	};

	serial@13800000 {
		status = "okay";
	};

	serial@13810000 {
		status = "okay";
	};

	serial@13820000 {
		status = "okay";
	};

	serial@13830000 {
		status = "okay";
	};

	fixed-rate-clocks {
		xxti {
			compatible = "samsung,clock-xxti";
			clock-frequency = <0>;
		};

		xusbxti {
			compatible = "samsung,clock-xusbxti";
			clock-frequency = <24000000>;
		};
	};
};
